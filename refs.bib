keywords = {Cross-flow shear method,Microfluidic device,Monodispersed droplet,Scaling law},
number = {6},
pages = {711--717},
title = {{Correlations of droplet formation in T-junction microfluidic devices: From squeezing to dripping}},
volume = {5},
year = {2008}
}
