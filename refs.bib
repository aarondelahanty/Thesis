doi = {10.1016/j.snb.2006.09.040},
file = {:C$\backslash$:/Users/Aaron Delahanty/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shui, Eijkel, van den Berg - 2007 - Multiphase flow in micro- and nanochannels.pdf:pdf},
